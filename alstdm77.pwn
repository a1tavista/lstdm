//------------------------------------------------------------------------------
function OnStatusUpdate()
{
    SetTimer("OnStatusUpdate", 1000, false);
    for(new p = 0; p < MAX_PLAYERS; p ++)
    {
        if(IsPlayerConnected(p))
        {
            if(ESCtime[p] > 60)
                if(!AFKed[p])
                    AFKed[p] = true;
            else if(ESCtime[p] <= 60)
                if(AFKed[p])
                    AFKed[p] = false;
            ESCtime[p] += 1;
            if(ESCtime[p] > 5)
            {
                new hh = floatround((ESCtime[p] - 1)/3600,floatround_floor);
                new mm = floatround((ESCtime[p] - 1)/60,floatround_floor) - 60*hh;
                new ss = (ESCtime[p] - 1) - 3600*hh - 60*mm;
                if(hh > 0)
                    format(Message,255,"Отсутствует %i:%02i:%02i",hh,mm,ss);
                else
                    format(Message,255,"Отсутствует %i:%02i",mm,ss);
                SetPlayerChatBubble(p,Message,COLOR_MAIN,50.0,1500);
            }
            else if(UpdatedChatBubble[p] != 0)
            {
                SetPlayerChatBubble(p, TempStatus[p], TempStatusC[p], 50.0, 1500);
                UpdatedChatBubble[p]--;
            }
            else if(strlen(PInfo[p][Status]) > 0)
                SetPlayerChatBubble(p,PInfo[p][Status], 0xFFFFFFAA, 50.0, 1500);
        }
    }
    return 1;
}

