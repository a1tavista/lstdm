forward CameraConnectPos();
public CameraConnectPos()
{
    Loop(playerid, MAX_PLAYERS)
    {
        if(Camtime[playerid] > 0)
        {
            CamX[playerid] -= 0.5;
            SetPlayerCameraPos(playerid,CamX[playerid],CamY[playerid],CamZ[playerid]);
            SetPlayerCameraLookAt(playerid,818.5380,-1402.7738,223.9858);
        }
    }
}
