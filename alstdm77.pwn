function IsPlayerNowAtDuel(playerid)
{
    if(PlayerDuel[playerid][PlayerDuelID] == 0) 
        return false;
    else 
        return true;
}

function SetToDuelPos(playerid, slot, DuelID)
{
    new string[256];
    if(DuelID > 0 && (Duel[DuelID][FirstSlot] == -1 || (Duel[DuelID][FirstSlot] >= 0 && Duel[DuelID][SecondSlot] >= 0)))
        return SendClientMessage(playerid, COLOR_SYSTEM, "[Ошибка!] Вы не можете присоединиться к данной дуэли.");
    if(DuelID == 0)
    {
        if(slot == 1)
        {
            for(new i=1; i<11; i++)
                if(Duel[i][FirstSlot] == -1 && Duel[i][Started] == 0 && Duel[i][Countdown] == 0)
                {
                    DuelID = i;
                    break;
                }            
        }            
        else if(slot == 2)
        {
            for(new i=1; i<11; i++)
                if(Duel[i][FirstSlot] != -1 && Duel[i][Started] == 0 && Duel[i][Countdown] == 0)
                {
                    DuelID = i;
                    break;
                }
        }
    }
    if(DuelID == 0)
        return SendClientMessage(playerid, COLOR_SYSTEM, "[Ошибка!] В данный момент нет активных дуэлей.");       
    SetPlayerVirtualWorld(playerid, DuelID + 10);
    if(slot == 1)
    {
        Duel[DuelID][FirstSlot] = playerid;
        SetPlayerPos(playerid,2539.7832,-1329.5649,23.4562);
        SetPlayerFacingAngle(playerid, 0.5880);
        SetPlayerTeam(playerid,playerid);
        SelectDuelWeapon(playerid);
        SetPlayerSkillLevel(playerid, 4, 999);
        SetPlayerSkillLevel(playerid, 5, 999);
        SetPlayerSkillLevel(playerid, 6, 999);
    } 
    else if(slot == 2)
    {
        format(string, sizeof(string), "* %s принял(а) дуэль №%d.", GetName(playerid), DuelID);
        SendClientMessageToAll(COLOR_DUEL, string);
        Duel[DuelID][SecondSlot] = playerid;
        SetPlayerPos(playerid,2538.3665,-1302.3743,23.4562);
        SetPlayerFacingAngle(playerid, 182.7514);
        SetPlayerTeam(playerid,playerid);
        SetPlayerSkillLevel(playerid, 4, 999);
        SetPlayerSkillLevel(playerid, 5, 999);
        SetPlayerSkillLevel(playerid, 6, 999);
        Duel[DuelID][Countdown] = 5;
        SetTimerEx("StartDuel", 1000, 0, "d", DuelID);
    }
    PlayerDuel[playerid][PlayerSlot] = slot;
    PlayerDuel[playerid][PlayerDuelID] = DuelID; 
    TogglePlayerControllable(playerid, 0);        
    SetCameraBehindPlayer(playerid);
    SetPlayerHealth(playerid,100.0);
    SetPlayerArmour(playerid,0.0);
    SpawnProtection[playerid] = 0;
    ResetPlayerWeapons(playerid);
    return 1;    
}

function StartDuel(duelid)
{
    if(Duel[duelid][Countdown] > 0)
    {       
        GameTextForPlayer(Duel[duelid][FirstSlot], CountText[Duel[duelid][Countdown]-1], 2000, 5);
        GameTextForPlayer(Duel[duelid][SecondSlot], CountText[Duel[duelid][Countdown]-1], 2000, 5);
        SetTimerEx("StartDuel", 1000, 0, "d", duelid);
    }
    if(Duel[duelid][Countdown] == 0)
    {
        Duel[duelid][Started] = 1;
        GivePlayerWeapon(Duel[duelid][FirstSlot], Duel[duelid][SelectedGun], 9999);
        GivePlayerWeapon(Duel[duelid][SecondSlot], Duel[duelid][SelectedGun], 9999);
        TogglePlayerControllable(Duel[duelid][FirstSlot], 1);
        TogglePlayerControllable(Duel[duelid][SecondSlot], 1);
        GameTextForPlayer(Duel[duelid][FirstSlot], "Fight!", 2000, 5);
        GameTextForPlayer(Duel[duelid][SecondSlot], "Fight!", 2000, 5);        
    } 
    Duel[duelid][Countdown]--;
    return 1;
}

function DisconnectFromDuel(playerid)
{
    new duelid = PlayerDuel[playerid][PlayerDuelID];
    new slot = PlayerDuel[playerid][PlayerSlot];
    if(slot == 1)
        EndDuel(duelid, Duel[duelid][SecondSlot]);
    else if(slot == 2) 
        EndDuel(duelid, Duel[duelid][FirstSlot]);
    return 1;
}

function EndDuel(duelid, reason)
{
    new string[128];

    switch(reason)
    {
        case -1: // отмена дуэли
        {
            format(string, sizeof(string), "* Дуэль №%d отменена.", duelid);
            SendClientMessageToAll(COLOR_DUEL, string);                     
        }
        default: // кто-то победил
        {
            GivePlayerMoney(reason, 100);
            format(string, sizeof(string), "* В дуэли №%d победил(а) %s.", duelid, GetName(reason));
            SendClientMessageToAll(COLOR_DUEL, string);
            PInfo[reason][Duels]+=1;                    
        }
    }

    new first = Duel[duelid][FirstSlot], second = Duel[duelid][SecondSlot];
    if(first != -1)
    {
        PlayerDuel[first][PlayerDuelID] = 0;
        PlayerDuel[first][PlayerSlot] = 0;
        if(reason == first || reason == -1) 
            SpawnPlayer(first); 
        SetPlayerVirtualWorld(first, 0);
    }
    if(second != -1)
    {
        PlayerDuel[second][PlayerDuelID] = 0;
        PlayerDuel[second][PlayerSlot] = 0;
        if(reason == second) 
            SpawnPlayer(second);
        SetPlayerVirtualWorld(second, 0);
    }
    Duel[duelid][FirstSlot] = -1;
    Duel[duelid][SecondSlot] = -1;
    Duel[duelid][Countdown] = 0;
    Duel[duelid][SelectedGun] = 0;
    Duel[duelid][Started] = 0;
    return 1;
}
