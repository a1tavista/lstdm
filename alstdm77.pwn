public OnPlayerDeath(playerid, killerid, reason) // #11
{
    new string[256];
    zoneupdates[playerid] = 0;
    player_zone[playerid] = -1;
    
       ContactPlayer[playerid][1]++;
    if(ContactPlayer[playerid][1] > 2)
    {
    KickMsg(playerid, "AntiCheat", "Накрутка EXP (FakeKill)", true);
    PInfo[killerid][Kills] -= 1;
    PInfo[killerid][BonusEXP] -= 1;
    }
    if(reason != 255 && reason != 47 && reason != 51 && reason != 53 && reason != 54 && ContactPlayer[killerid][0] != playerid)
    {
    ContactPlayer[killerid][0] = INVALID_PLAYER_ID;
    ContactPlayer[playerid][0] = INVALID_PLAYER_ID;
    KickMsg(playerid, "AntiCheat", "Накрутка EXP (FakeKill)", true);
    PInfo[killerid][Kills] -= 1;
    PInfo[killerid][BonusEXP] -= 1;
    }
    
    ServerStats[TotalDeaths] += 1;
    if(DM[playerid][fd]!=1) PInfo[playerid][Deaths] += 1;

    if(Joined[playerid] == true)
    {
        if(OnRace[playerid] == true)
        {
            RaceSpawnPos[playerid] = 0;
            JoinCount--;
            Joined[playerid] = false;
            OnRace[playerid] = false;
            DestroyVehicle(CreatedRaceVeh[playerid]);
            DisablePlayerRaceCheckpoint(playerid);
            TextDrawHideForPlayer(playerid, RaceInfo[playerid]);
            CPProgess[playerid] = 0;
            CPCoords[CPProgess[playerid]][3] = 0;
            KillTimer(InfoTimer[playerid]);
            format(string,sizeof(string),"* %s выбыл(а) из гонки.",GetName(playerid));
            SendClientMessageToAll(COLOR_RACE,string);
            SetPlayerVirtualWorld(playerid, 0);
        }
    }
    if(gTeam[playerid]==TEAM_MEDICS)
    {
        ItemInfo[playerid][Item1n] = 0;
        ItemInfo[playerid][Item1ID] = 0;
    }
    if(TaxiCall[playerid])
    {
        DisableMarker(playerid, -1, TEAM_TAXI, 1);
        TaxiCall[playerid] = 0;
    }
    if(MedCall[playerid])
    {
        DisableMarker(playerid, -1, TEAM_MEDICS, 1);
        MedCall[playerid] = 0;
    }

    ForAllPlayers(x) 
        if(GetPlayerState(x) == PLAYER_STATE_SPECTATING && gSpectateID[x] == playerid)
        {
            StopSpectate(x);
            spectating[x] = 0;
            if(DM[x][ad]==1 || DM[x][bd]==1)
                SetPlayerVirtualWorld(x, 2);
            else if(DM[x][cd]==1 || DM[x][dd]==1)
                SetPlayerVirtualWorld(x, 3);
            else if(DM[x][ed]==1 || DM[x][fd]==1)
                SetPlayerVirtualWorld(x, 4);
            SetPlayerPos(x,AdminCoords[x][14][ax],AdminCoords[x][14][ay],AdminCoords[x][14][az]);
            SetPlayerFacingAngle(x,AdminCoords[x][14][Angle]);
            SetPlayerInterior(x,AdminCoords[x][14][Interior]);
        }

    CreatePlayerWeaponPickup(playerid);

    if(killerid != INVALID_PLAYER_ID)
    {
        SendDeathMessage(killerid,playerid,reason);
        if(IsPlayerInAnyVehicle(killerid) && GetPlayerState(killerid) == PLAYER_STATE_DRIVER && Killed[playerid]==0)
            if(reason == 50) 
                KickMsg(killerid, "AntiCheat", "Убийство лопостями вертолета", true);
        if(gTeam[killerid]!=TEAM_POLICE && DM[killerid][ad]==0 && DM[killerid][bd]==0 && DM[killerid][cd]==0 && DM[killerid][dd]==0 && DM[killerid][ed]==0 && DM[killerid][fd]==0 && !IsPlayerNowAtDuel(killerid)) Spree[killerid]++;
        if((Spree[killerid] == 3 || Spree[killerid] == 6 || Spree[killerid] == 9 || Spree[killerid] == 12 || Spree[killerid] == 15 || Spree[killerid] == 18))
        {
            format(string, sizeof(string), "Внимание всем постам! Разыскивается %s (ID %d) за совершение %d убийств подряд! Награда: %d$.",GetName(killerid),killerid,Spree[killerid],Spree[killerid]*1000);
            SendPoliceMessage(string);
            SetPlayerWantedLevel(killerid, Spree[killerid] / 3);
        }
        if(Spree[playerid]>=3)
        {
            if(gTeam[killerid] == TEAM_POLICE)
            {
                format(string, sizeof(string), "%s успешно нейтрализовал(а) преступника %s.", GetName(killerid),GetName(playerid));
                SendPoliceMessage(string);
                PInfo[killerid][PoliceScore]+=Spree[playerid];
                A_GivePlayerMoney(killerid, STOP_KILLING_SPREE_COST);
            }
            Spree[playerid] = 0;
            if(GetPlayerWantedLevel(playerid) > 0) SetPlayerWantedLevel(playerid, 0);
        }
        if((gTeam[killerid]==TEAM_CIVILIANS || gTeam[killerid]==TEAM_GROOVE || gTeam[killerid]==TEAM_BALLAS || gTeam[killerid]==TEAM_AZTECAS || gTeam[killerid]==TEAM_THELOST || gTeam[killerid]==TEAM_MKINGS || gTeam[killerid]==TEAM_RUSSIANS || gTeam[killerid]==TEAM_TRIADS || gTeam[killerid]==TEAM_POLICE || gTeam[killerid]==TEAM_NOD || gTeam[killerid]==TEAM_ELITE || gTeam[killerid]==TEAM_A7 || gTeam[killerid]==TEAM_TSOD || gTeam[killerid]==TEAM_MTD) && gTeam[playerid] != TEAM_MEDICS && DM[killerid][fd]!=1)
            PInfo[killerid][Kills]+=1;
        if((gTeam[killerid]==TEAM_GROOVE || gTeam[killerid]==TEAM_BALLAS || gTeam[killerid]==TEAM_AZTECAS || gTeam[killerid]==TEAM_THELOST || gTeam[killerid]==TEAM_MKINGS || gTeam[killerid]==TEAM_RUSSIANS || gTeam[killerid]==TEAM_TRIADS || gTeam[killerid]==TEAM_NOD || gTeam[killerid]==TEAM_ELITE || gTeam[killerid]==TEAM_A7 || gTeam[killerid]==TEAM_TSOD || gTeam[killerid]==TEAM_MTD) && gTeam[playerid] != TEAM_MEDICS && DM[killerid][fd]!=1)
            PInfo[killerid][BonusEXP]+=1;
        if(gTeam[killerid]==TEAM_MEDICS && (DM[killerid][ad]==0 || DM[killerid][bd]==0 || DM[killerid][cd]==0 || DM[killerid][dd]==0 || DM[killerid][ed]==0 || DM[killerid][fd]==0 || !IsPlayerNowAtDuel(killerid)))
            PInfo[killerid][MedScore]-=1;
        if(PInfo[playerid][Bounty] > 0) 
        {
            format(string, sizeof(string), "* Вы получили %d$ за выполнение контракта на убийство %s.", PInfo[playerid][Bounty],GetName(playerid));
            SendClientMessage(killerid, COLOR_DUEL, string);
            A_GivePlayerMoney(killerid, PInfo[playerid][Bounty]);
            PInfo[playerid][Bounty] = 0;
        }
        A_GivePlayerMoney(killerid, KILL_MONEYS);
        ServerStats[TotalKills] += 1;
        if(PlayerDuel[playerid][PlayerDuelID] != 0)
        {  
            EndDuel(PlayerDuel[playerid][PlayerDuelID], killerid);
            SetPlayerVirtualWorld(playerid, 0);
            PlayerDuel[playerid][PlayerDuelID] = 0;
            PlayerDuel[playerid][PlayerSlot] = 0;
            PlayerDuel[playerid][PlayerState] = 0;
        }
    } 
    else 
        SendDeathMessage(INVALID_PLAYER_ID,playerid,reason);
    return 1;
}
