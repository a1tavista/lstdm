stock KitTimer(playerid, type)
{
    if(KitTimer_Var[playerid][K_Count] > 0)
        return SendClientMessage(playerid, COLOR_SYSTEM, "[Ошибка!] Таймер использования уже запущен.");
    
    new Float:X, Float:Y, Float:Z, Float:HP; 
    GetPlayerPos(playerid, X, Y, Z);
    GetPlayerHealth(playerid, HP);
    
    KitTimer_Var[playerid][K_XCoords] = X;
    KitTimer_Var[playerid][K_YCoords] = Y; 
    KitTimer_Var[playerid][K_ZCoords] = Z; 
    KitTimer_Var[playerid][K_Health] = HP;
    KitTimer_Var[playerid][K_Type] = type;
    
    if(type == 1 && PInfo[playerid][LVL] < 5)
    {
        SetTimerEx("UseApt", 1000, 0, "d", playerid);
        PUsed[playerid][0] = 20;
        SetBubbleText(playerid, COLOR_MEDICS, "Использует аптечку...", 1);
    } 
    else if(type == 2 && PInfo[playerid][LVL] < 5)
    {
        SetTimerEx("UseArm", 1000, 0, "d", playerid);
        PUsed[playerid][4] = 20;
        SetBubbleText(playerid, COLOR_DUEL, "Использует бронекомплект...", 1);  
    } 
    
    if(PInfo[playerid][LVL] < 4)
        KitTimer_Var[playerid][K_Count] = 5;
    else if(PInfo[playerid][LVL] == 4)
        KitTimer_Var[playerid][K_Count] = 4;
    else if(PInfo[playerid][LVL] > 4)
    {
        UseKit(playerid);
        KitTimer_Var[playerid][K_Count] = 0;
    }
           
    if(KitTimer_Var[playerid][K_Count] > 0) 
    {
        SendClientMessage(playerid, COLOR_SYSTEM, "* Не двигайтесь, пока идет отсчет, чтобы использовать предмет.");
        KitTimer_Var[playerid][K_Timer] = SetTimerEx("KitTimerCount", 1000, 0, "d", playerid);
    }
    return 1;
}

function KitTimerCount(playerid)
{
    new string[256]; 
    format(string, sizeof(string), "%d...", KitTimer_Var[playerid][K_Count]);
    SendClientMessage(playerid, COLOR_SYSTEM, string);     
    if(KitTimer_Var[playerid][K_Count] > 1) 
    {
        new Float:X, Float:Y, Float:Z, Float:HP; 
        GetPlayerPos(playerid, X, Y, Z);
        GetPlayerHealth(playerid, HP);
        if(IsPlayerInRangeOfPoint(playerid, 0.5, KitTimer_Var[playerid][K_XCoords], KitTimer_Var[playerid][K_YCoords], KitTimer_Var[playerid][K_ZCoords]))
        {
            KitTimer_Var[playerid][K_Count]--;
            if(KitTimer_Var[playerid][K_Type] == 1)
            {
                SetBubbleText(playerid, COLOR_MEDICS, "Использует аптечку...", 1);
            } 
            else if(KitTimer_Var[playerid][K_Type] == 2)
            {
                SetBubbleText(playerid, COLOR_DUEL, "Использует бронекомплект...", 1);  
            }             
            KitTimer_Var[playerid][K_Timer] = SetTimerEx("KitTimerCount", 1000, 0, "d", playerid);
        }
        else
        {
            KitTimer_Var[playerid][K_Count] = 0;
            SendClientMessage(playerid, COLOR_SYSTEM, "* Процесс использования прерван.");
