function CountDown()
{
if (Count > 0)
{
GameTextForAll(CountText[Count-1],2000, 5);
Count--;
SetTimer("CountDown",1000,0);
}
else
{
GameTextForAll("~r~~h~GO GO GO!",2000, 5);
Count = 5;
}
return 1;
}
//------------------------------------------------------------------------------
function DoesPlayerHasTag(playerid,tag[])
{
 new name[MAX_PLAYER_NAME]; GetPlayerName(playerid,name,sizeof(name));
 return (strfind(name,tag,true) != -1) ? 1 : 0;
} 
//------------------------------------------------------------------------------
function Encrypt(string[])
{
for(new x=0; x < strlen(string); x++)
  {
  string[x] += (3^x) * (x % 15);
  if(string[x] > (0xff))
  {
  string[x] -= 64;
  }
  }
return 1;
}
//------------------------------------------------------------------------------
function UpdateServerStats()
{
for(new i=0; i<2000; i++)
{
SetVehicleParamsEx(i,-1,-1,-1,VEHICLE_PARAMS_OFF,-1,-1,-1);
}
new query[256];
format(query, sizeof(query), "UPDATE `tdm_server_stats` SET `TotalKills`='%d', `TotalDeaths`='%d', `TotalMoneyGives`='%d', `TotalMoneyLoses`='%d', `TotalMoneyRecieved`='%d', `TotalKicked`='%d', `TotalBanned`='%d', `MaxPlayers`='%d'",
ServerStats[TotalKills], ServerStats[TotalDeaths], ServerStats[TotalMoneyGives], ServerStats[TotalMoneyLoses],
ServerStats[TotalMoneyRecieved], ServerStats[TotalKicked], ServerStats[TotalBanned], ServerStats[MaxPlayers]);
mysql_query(query);
return 1;
}

