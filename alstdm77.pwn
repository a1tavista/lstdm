    while(!IsPlayerConnected(rand) || rand == id || GetPlayerState(rand) == PLAYER_STATE_SPECTATING && gSpectateID[rand] != INVALID_PLAYER_ID || GetPlayerState(rand) != 1 && GetPlayerState(rand) != 2 && GetPlayerState(rand) != 3);
