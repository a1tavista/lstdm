function OpenInventory(playerid)
{
    new MedKitCountdown[70], ArmKitCountdown[70], RepairKitCountdown[70], string[256];
    if(PUsed[playerid][0] > 0)
        format(MedKitCountdown, 70, ", перезарядка: %d сек.", PUsed[playerid][0]);
    if(PUsed[playerid][4] > 0)
        format(ArmKitCountdown, 70, ", перезарядка: %d сек.", PUsed[playerid][4]);
    if(PUsed[playerid][1] > 0)
        format(RepairKitCountdown, 70, ", перезарядка: %d сек.", PUsed[playerid][1]);
    format(string,256,"%s (x%d%s)\n%s (x%d%s)\n%s (x%d%s)",
    Item1Names[ItemInfo[playerid][Item1ID]],ItemInfo[playerid][Item1n], MedKitCountdown,
    Item2Names[ItemInfo[playerid][Item2ID]],ItemInfo[playerid][Item2n], ArmKitCountdown,
    Item3Names[ItemInfo[playerid][Item3ID]],ItemInfo[playerid][Item3n], RepairKitCountdown);
    ShowPlayerDialog(playerid, 201, DIALOG_STYLE_LIST, "{DCDCDC}= LSTDM 7.5b by NOD-Team | Инвентарь =", string, "Выбрать", "Назад");
    return 1;
}

