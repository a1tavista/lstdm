//------------------------------------------------------------------------------
function CarSpawner(playerid,model)
{
    if(IsPlayerInAnyVehicle(playerid))
        RemovePlayerFromVehicle(playerid);
    if(AdminVehicle[playerid] != 0)
    {
        DestroyObject(Neon[AdminVehicle[playerid]][LeftNeon]);
        DestroyObject(Neon[AdminVehicle[playerid]][RightNeon]);
        DestroyObject(BFNeon[AdminVehicle[playerid]][0]);
        DestroyObject(BFNeon[AdminVehicle[playerid]][1]);
        DestroyVehicle(AdminVehicle[playerid]);
    }
    AdminVehicle[playerid] = 0;
    new Float:X, Float:Y, Float:Z, Float:R;
    GetPlayerPos(playerid, X, Y, Z);
    GetPlayerFacingAngle(playerid, R);
    new id = CreateVehicle(model, X, Y, Z, R, -1, -1, 99999);
    AdminVehicle[playerid] = id;
    new string3[50];
    format(string3, sizeof(string3), "%c%c%c %i%i%i", (65+random(26)), (65+random(26)), (65+random(26)), random(10), random(10), random(10));
    SetVehicleNumberPlate(id, string3);

        DestroyObject(Neon[id][LeftNeon]);
        DestroyObject(Neon[id][RightNeon]);
        DestroyObject(BFNeon[id][0]);
        DestroyObject(BFNeon[id][1]);

    PutPlayerInVehicle(playerid,AdminVehicle[playerid], 0);
    LinkVehicleToInterior(AdminVehicle[playerid], GetPlayerInterior(playerid));
    new world = GetPlayerVirtualWorld(playerid);
    SetVehicleVirtualWorld(id, world);
}
//------------------------------------------------------------------------------
stock A_GivePlayerMoney(playerid, money, bool:recieved=false)
{
    PInfo[playerid][Money] += money;
    if(recieved==false) 
    {
        if(money>0) 
            ServerStats[TotalMoneyGives]+=money;
        else 
            ServerStats[TotalMoneyLoses]+=money;
    }
    return A_SetPlayerMoney(playerid, PInfo[playerid][Money]);
}
//------------------------------------------------------------------------------
stock A_SetPlayerMoney(playerid, money)
{
    PInfo[playerid][Money] = money;
    ResetPlayerMoney(playerid);
    return GivePlayerMoney(playerid,money);
}
//------------------------------------------------------------------------------
stock A_GetPlayerMoney(playerid)
{
    return PInfo[playerid][Money];
}
//------------------------------------------------------------------------------
stock A_ResetPlayerMoney(playerid)
{
    PInfo[playerid][Money] = 0;
    ResetPlayerMoney(playerid);
    return 1;
}
//------------------------------------------------------------------------------
public OnPlayerEnterRaceCheckpoint(playerid)
{
    if(CPProgess[playerid] == TotalCP -1)
    {
        new TimeStamp, TotalRaceTime, string[256], rFile[256], pName[MAX_PLAYER_NAME], Prize[2], TempTotalTime;
        Position++;
        CPProgess[playerid]++;
        GetPlayerName(playerid, pName, sizeof(pName));
        TimeStamp = GetTickCount();
        TotalRaceTime = TimeStamp - RaceTick;
        switch(Position)
        {
            case 1:
            {
            PInfo[playerid][Races]+=1;
            PInfo[playerid][RacesW]+=1;
            if(gTeam[playerid] == TEAM_RACERS) Prize[0] = 13000, Prize[1] = 8;
            else Prize[0] = 10000, Prize[1] = 6;
            }
            case 2:
            {
                PInfo[playerid][Races]+=1;
                if(gTeam[playerid] == TEAM_RACERS){
                Prize[0] = 10000; Prize[1] = 7;
                }else{ Prize[0] = 8000; Prize[1] = 5;}
            }
            case 3:
            {
                PInfo[playerid][Races]+=1;
                if(gTeam[playerid] == TEAM_RACERS){
                Prize[0] = 8000; Prize[1] = 6;
                }else{ Prize[0] = 5000; Prize[1] = 4;}
            }
            default:
            {
            PInfo[playerid][Races]+=1;
            if(gTeam[playerid] == TEAM_RACERS) Prize[0] = 4000, Prize[1] = 4;
            else Prize[0] = 3000, Prize[1] = 2;
            }
        }
        format(string, sizeof(string), "* %s финишировал(а) %d в гонке за %s.", pName, Position, ConvertSeconds(TotalRaceTime));
        SendClientMessageToAll(COLOR_RACE, string);

        format(string, sizeof(string), "* Вы получаете %d$ и %d EXP. Поздравляем!", Prize[0], Prize[1]);
        SendClientMessage(playerid, COLOR_RACE, string);
        SetPlayerPos(playerid, AdminCoords[playerid][15][ax], AdminCoords[playerid][15][ay], AdminCoords[playerid][15][az]);
        SetPlayerInterior(playerid, AdminCoords[playerid][15][Interior]);
        DestroyVehicle(CreatedRaceVeh[playerid]);
        Joined[playerid] = false;
        RaceSpawnPos[playerid] = 0;
        OnRace[playerid] = false;
        TextDrawHideForPlayer(playerid, RaceInfo[playerid]);
        CPProgess[playerid] = 0;
        CPCoords[CPProgess[playerid]][3] = 0;
        KillTimer(InfoTimer[playerid]);
        SetPlayerVirtualWorld(playerid, 0);
        if(FinishCount == 0)
        {
            format(rFile, sizeof(rFile), "/rRaceSystem/%s.RRACE", RaceName);
            format(string, sizeof(string), "BestRacerTime_%d", TimeProgress);
            TempTotalTime = dini_Int(rFile, string);
            if(TotalRaceTime <= dini_Int(rFile, string) || TempTotalTime == 0)
            {
                  new TimeRaceRecord = TotalRaceTime - TempTotalTime;
                  dini_IntSet(rFile, string, TotalRaceTime);
                format(string, sizeof(string), "BestRacer_%d", TimeProgress);
                if(TempTotalTime != 0)
                    format(string, sizeof(string), "* %s опередил(а) рекорд %s на %s.", pName, dini_Get(rFile, string), ConvertSeconds(-TimeRaceRecord));
                else
                    format(string, sizeof(string), "* %s поставил(а) новый рекорд.", pName);
                SendClientMessageToAll(COLOR_RACE, string);
                format(string, sizeof(string), "BestRacer_%d", TimeProgress);
                dini_Set(rFile, string, pName);
                TimeProgress++;
                PInfo[playerid][BonusEXP] += 2;
            }
        }
        JoinCount--;
        FinishCount++;
        A_GivePlayerMoney(playerid, Prize[0]);
        PInfo[playerid][BonusEXP] += Prize[1];
        DisablePlayerRaceCheckpoint(playerid);
        if(JoinCount <= 0 && FinishCount >= 0)
        {
            SendClientMessageToAll(COLOR_RACE, "* Гонка завершена.");
            StopRace(1);
        }
    }
    else
    {
        CPProgess[playerid]++;
        CPCoords[CPProgess[playerid]][3]++;
        RacePosition[playerid] = floatround(CPCoords[CPProgess[playerid]][3], floatround_floor);
        SetCP(playerid, CPProgess[playerid], CPProgess[playerid]+1, TotalCP, RaceType);
        PlayerPlaySound(playerid, 1137, 0.0, 0.0, 0.0);
    }
    return 1;
}

