function IsPlayerAtAnyDM(playerid)
{
    return DM[playerid][ad] == 1 || DM[playerid][bd] == 1 || DM[playerid][cd] == 1 || DM[playerid][dd] == 1 || DM[playerid][ed] == 1 || DM[playerid][fd] == 1 ? true : false;
}

function IsPlayerAtDM(playerid, dmid)
{
    switch(dmid)
    {
        case 1: return DM[playerid][ad] == 1 ? true : false;
        case 2: return DM[playerid][bd] == 1 ? true : false; 
        case 3: return DM[playerid][cd] == 1 ? true : false; 
        case 4: return DM[playerid][dd] == 1 ? true : false;
        case 5: return DM[playerid][ed] == 1 ? true : false;
        case 6: return DM[playerid][fd] == 1 ? true : false;
    }
    return false;
}
