function settime() // #2
{
    new hour,minute,second;
    new string[256];
    gettime(hour,minute,second);
    if (minute <= 9)
        format(string,25,"%d:0%d",hour,minute);
    else 
        format(string,25,"%d:%d",hour,minute);
    TextDrawSetString(Clock,string);
    return 1;
}

