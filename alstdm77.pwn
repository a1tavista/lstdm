public OnPlayerText(playerid, text[]) // #12
{
    new name[24], string[256];
    GetPlayerName(playerid, name, 24);

    if(small[playerid] == 1)
    {
        new caps;
        while (text[++caps])
        {
        if('A' <= text[caps] <= 'Z') text[caps] |= 0x20;
        if('А' <= text[caps] <= 'Я') text[caps] |= 0x20;
        }
    }

    if(stringContainsIP(text))
        if(gTeam[playerid] != TEAM_NOD && PInfo[playerid][LVL]<5) return KickMsg(playerid, "AntiCheat", "Реклама", true), false;

    if(IsPlayerInAnyVehicle(playerid))
    {
        if(text[0] == '@')
        {
            format(string, sizeof(string), "[Транспорт]: %s (%d): %s", name, playerid, text[1]);
            for(new i = 0; i < MAX_PLAYERS; i++)
                if(IsPlayerConnected(i))
                    if(GetPlayerVehicleID(playerid) == GetPlayerVehicleID(i))
                        SendClientMessage(i, GetPlayerColor(playerid), string), printf("%s", string);     
            return 0;
        }
    }

    if(text[0] == '!')
    {
        format(string, sizeof(string), "[Команда]: %s (%d): %s",name, playerid, text[1]);
        for(new i = 0; i < MAX_PLAYERS; i++)
            if(IsPlayerConnected(i))
                if(gTeam[i] == gTeam[playerid] && gPlayerSpawned[i] == 1 || gSpectateID[i] == playerid)
                    SendClientMessage(i, GetPlayerColor(playerid), string);
        return 0;
    }

    if(muted[playerid] != 0)
    {
        format(string, sizeof(string), "[Ошибка!] Вам временно заблокирован доступ в чат! До снятия блокировки осталось %d сек.", muted[playerid]);
        SendClientMessage(playerid, COLOR_SYSTEM, string);
        return 0;
    }

    if(blockchat == 1 && PInfo[playerid][LVL] < 5)
    {
        SendClientMessage(playerid, COLOR_SYSTEM, "[Ошибка!] Чат временно заблокирован.");
        return 0;
    }

    if(PInfo[playerid][Logged] == 0)
    {
        SendClientMessage(playerid, COLOR_SYSTEM, "[Ошибка!] Чтобы писать в чат, необходимо зарегистрироваться либо авторизоваться.");
        return 0;
    }

    SendMessage(playerid, text);
    return 0;
}

function SendMessage(playerid,text[])
{
    if(IsPlayerFlooding(playerid))  
        return SendClientMessage(playerid, COLOR_SYSTEM, "[Ошибка!] Вы можете отправить не более одного сообщения раз в две секунды.");
    iPlayerChatTime[playerid] = GetTickCount();

    new ttext[256];
    if(strlen(text)>72)
    {
        strmid(ttext, text, 0, 72);
        format(ttext, sizeof(ttext), "%s (%d): {ffffff}%s", GetName(playerid), playerid, ttext);
        SendMessageToChannel(playerid, GetPlayerColor(playerid), ttext);
        strmid(ttext, text, 72, 128);
        format(ttext, sizeof(ttext), "{ffffff}%s", ttext);
        SendMessageToChannel(playerid, GetPlayerColor(playerid), ttext);
    }
    else
    {
        format(ttext, sizeof(ttext), "%s (%d): {ffffff}%s", GetName(playerid), playerid, text);
        SendMessageToChannel(playerid, GetPlayerColor(playerid), ttext);
    }
    return 0;
}

function SendMessageToChannel(playerid, color, text[])
{
    for(new i=0; i<MAX_PLAYERS; i++)
        if(Channel[playerid] == Channel[i])
            SendClientMessage(i, GetPlayerColor(playerid), text);
        }

new TempStatus[MAX_PLAYERS][MAX_STRING];
new TempStatusC[MAX_PLAYERS];

function SetBubbleText(playerid, color, text[], time)
{
    UpdatedChatBubble[playerid] = time;
    format(TempStatus[playerid], MAX_STRING, text);
    TempStatusC[playerid] = color;
