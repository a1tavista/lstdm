//------------------------------------------------------------------------------
stock ConvertSeconds(Time,MSEx = 1)
{
 new string[128];
 new M, S, Ms;
 if(Time > 0)
 {
  new Float:fTime = floatdiv(Time, 60000);
  M = floatround(fTime, floatround_tozero);
  S = floatround(floatmul(fTime - M, 60), floatround_tozero);
  Ms = floatround(floatmul(floatmul(fTime - M, 60) - S, 1000), floatround_tozero);
  if(MSEx == 1) format(string,sizeof(string),"%02d:%02d.%03d",M,S,Ms);
  else if(MSEx == 0) format(string,sizeof(string),"%02d:%02d",M,S);
 }
 else if(Time == 0) format(string,sizeof(string),"00:00",M,S,Ms);
 return string;
}
