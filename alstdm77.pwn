//------------------------------------------------------------------------------
stock strrest(const string[], &index, const splitter = ' ')
{
    new length = strlen(string);
    while ((index < length) && (string[index] <= splitter))
        index++;
    new offset = index;
    new result[128];
    while ((index < length) && ((index - offset) < (sizeof(result) - 1)))
    {
        result[index - offset] = string[index];
        index++;
    }
    result[index - offset] = EOS;
    return result;
}

