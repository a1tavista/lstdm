public OnRconLoginAttempt(ip[], password[], success)
{
new str[256];
new pip[25];
new count;
if(!success)
{
for(new i=0; i<GetMaxPlayers(); i++)
{
if(IsPlayerConnected(i))
{
GetPlayerIp(i, pip, sizeof(pip));
if(!strcmp(ip, pip, true))
{
count++;
Attempt[i]++;
format(str,256,"* %s (%d) - попытка взлома RCON: %d/3. Вводимый пароль: %s.",GetName(i),i,Attempt[i],password);
SendMessageToAdmins(str,1);
if(Attempt[i]==3) KickMsg(i, "AntiCheat", "Попытка взлома RCON", true);
}
}
}
if(count==0)
{
 format(str,256,"* Попытка взлома RCON из консоли. IP: %s. Вводимый пароль: %s.",ip,password);
SendMessageToAdmins(str,1);
}
}
return 1;
}

