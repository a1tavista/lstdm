static iPlayerChatTime[MAX_PLAYERS];
stock IsPlayerFlooding(playerid)
{
    if(GetTickCount() - iPlayerChatTime[playerid] < 2000)
        return 1;
    return 0;
}

stock stringContainsIP(string[])
{
    new dotCount;
    for(new i; string[i] != EOS; ++i)
        if(('0' <= string[i] <= '9') || string[i] == '.' || string[i] == ',' || string[i] == ';' || string[i] == ''' || string[i] == ':')
            if((string[i] == '.' || string[i] == ',' || string[i] == ';' || string[i] == ''') && (string[i + 1] != '.' || string[i + 1] != ',' || string[i + 1] != ';' || string[i + 1] != ',' || string[i + 1] != ''') && ('0' <= string[i - 1] <= '9'))
                ++dotCount;
    return (dotCount > 2);
}

