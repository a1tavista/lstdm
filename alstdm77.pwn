function LoginAccount(playerid, password[])
{
    new query[256];
    new string[256];
    new field[50];
    new esc_pass[2][50];
    strmid(esc_pass[0], password, 0, 50);
    mysql_real_escape_string(esc_pass[0], esc_pass[1]);
    format(query,sizeof(query),"SELECT * FROM tdm_players WHERE `nickname` = '%s' AND `password` = md5('%s') LIMIT 1", PNickWithoutTag[playerid], esc_pass[1]);
    mysql_query(query);
    mysql_store_result();
    if(!mysql_num_rows())
    {
        if(LoginAttempts[playerid]>=3)
            KickMsg(playerid,"AntiCheat","Неудачная авторизация",true);
        else
        {
            format(string,256,"{DCDCDC}= LSTDM %s by NOD-Team | Ошибка =",Version);
            ShowPlayerDialog(playerid, 252, DIALOG_STYLE_INPUT, string, "{FFFFF0}Вы ввели неверный пароль.\nВ случае повторения неудачных попыток вы будете кикнуты системой защиты.\n\nПопробуйте ввести правильный пароль:", "Войти", "Отмена");
            LoginAttempts[playerid]++;
        }
    }
    else
    {
        new Banned;
        mysql_fetch_field("banned", field);
        Banned = strval(field);
        if(Banned == 1)
        {
            KickMsg(playerid,"AntiCheat","Аккаунт заблокирован",true);
            Banned = 0;
        }
        else
        {
            PInfo[playerid][Logged] = 1;
            mysql_fetch_field("kills", field);
            PInfo[playerid][Kills] = strval(field);

            mysql_fetch_field("muted", field);
            muted[playerid] = strval(field);
            if(muted[playerid]!=0)
            {
            SetTimerEx("PlayerUnMute",1000,0,"d",playerid);
            }

            mysql_fetch_field("jailed", field);
            jailed[playerid] = strval(field);
            if(jailed[playerid]!=0)
            {
            SetTimerEx("PlayerUnJail",1000,0,"d",playerid);
            }

            mysql_fetch_field("deaths", field);
            PInfo[playerid][Deaths] = strval(field);

            mysql_fetch_field("lvl", field);
            PInfo[playerid][LVL] = strval(field);

            mysql_fetch_field("money", field);
            A_SetPlayerMoney(playerid, strval(field));

            mysql_fetch_field("bounty", field);
            PInfo[playerid][Bounty] = strval(field);

            mysql_fetch_field("bonus_exp", field);
            PInfo[playerid][BonusEXP] = strval(field);

            mysql_fetch_field("races", field);
            PInfo[playerid][Races] = strval(field);

            mysql_fetch_field("race_wins", field);
            PInfo[playerid][RacesW] = strval(field);

            mysql_fetch_field("duels", field);
            PInfo[playerid][Duels] = strval(field);

            mysql_fetch_field("kicks", field);
            PInfo[playerid][Kicks] = strval(field);

            mysql_fetch_field("bans", field);
            PInfo[playerid][Bans] = strval(field);

            mysql_fetch_field("player_id", field);
            PInfo[playerid][ServerID] = strval(field);

            mysql_fetch_field("marry", field);
            PInfo[playerid][MarrySID] = strval(field);

            mysql_fetch_field("score1", field);
            PInfo[playerid][MedScore] = strval(field);

            mysql_fetch_field("score2", field);
            PInfo[playerid][PoliceScore] = strval(field);

            mysql_fetch_field("score3", field);
            PInfo[playerid][TaxiScore] = strval(field);

            mysql_fetch_field("rang1", field);
            PInfo[playerid][MedRank] = strval(field);

            mysql_fetch_field("rang2", field);
            PInfo[playerid][PoliceRank] = strval(field);

            mysql_fetch_field("rang3", field);
            PInfo[playerid][TaxiRank] = strval(field);

            GetPlayerIp(playerid,PInfo[playerid][IP],50);

            mysql_fetch_field("Item1", field);
            ItemInfo[playerid][Item1ID] = strval(field);

            mysql_fetch_field("Item1n", field);
            ItemInfo[playerid][Item1n] = strval(field);

            mysql_fetch_field("Item2", field);
            ItemInfo[playerid][Item2ID] = strval(field);

            mysql_fetch_field("Item2n", field);
            ItemInfo[playerid][Item2n] = strval(field);

            mysql_fetch_field("Item3", field);
            ItemInfo[playerid][Item3ID] = strval(field);

            mysql_fetch_field("Item3n", field);
            ItemInfo[playerid][Item3n] = strval(field);

            mysql_fetch_field("realname", field);
            UserInfo[playerid][Name] = field;

            mysql_fetch_field("is_female", field);
            UserInfo[playerid][Sex] = strval(field);

            mysql_fetch_field("skype", field);
            UserInfo[playerid][Skype] = field;

            mysql_fetch_field("vkontakte", field);
            UserInfo[playerid][VK] = field;

            mysql_fetch_field("city", field);
            UserInfo[playerid][City] = field;

            mysql_fetch_field("icq", field);
            UserInfo[playerid][ICQ] = field;
            PInfo[playerid][EXP] = PInfo[playerid][BonusEXP]+PInfo[playerid][MedScore]+PInfo[playerid][TaxiScore]+PInfo[playerid][PoliceScore]+PInfo[playerid][Kills];
            format(query,sizeof(query),"UPDATE `tdm_players` SET `last_join`='NOW()' WHERE `player_id` = '%d' LIMIT 1", PInfo[playerid][ServerID]);
            mysql_query(query);
            mysql_error(string);
            if(strlen(string) > 1) SendClientMessageToAll(COLOR_DUEL, string);
            mysql_free_result();
        }
    }
    mysql_free_result();   
    if(PInfo[playerid][MarrySID] != 0)
    {
        format(query,sizeof(query),"SELECT lastnickname FROM tdm_players WHERE `player_id` = '%d' LIMIT 1", PInfo[playerid][MarrySID]);
