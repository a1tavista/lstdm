function CountTillRace()
{
switch(CountAmount)
{
    case 0:
    {
        new string[128];
        if(JoinCount>=2) 
            StartRace(), format(string, sizeof(string), "* Гонка %s началась!", RaceName), SendClientMessageToAll(COLOR_RACE, string);
        else
        {
            StopRace(0);
            SendClientMessageToAll(COLOR_RACE, "* Гонка отменена, т.к. не набралось достаточное количество участвующих.");
        }
    }
    case 1..5:
    {
        new string[10];
        format(string, sizeof(string), "~r~%d", CountAmount);
        ForEach(i, MAX_PLAYERS)
        {
            if(Joined[i] == true)
            {
                GameTextForPlayer(i, string, 999, 5);
                PlayerPlaySound(i, 1056, 0.0, 0.0, 0.0);
                SetCameraBehindPlayer(i);
            }
        }
    }
    case COUNT_DOWN_TILL_RACE_START:
    {
        new string[128];
        format(string, sizeof(string), "* Гонка %s начнется через %d секунд.", RaceName, CountAmount);
        SendClientMessageToAll(COLOR_RACE, string);
        format(string, sizeof(string), "* Транспорт: %s | Автор: %s | Рекорд: %s - %s.", Veh, Author, BestRacer, ConvertSeconds(BestRacerTime));
        SendClientMessageToAll(COLOR_RACE, string);
        format(string, sizeof(string), "* Введите /race, если желаете принять участие.");
        SendClientMessageToAll(COLOR_RACE, string);
    }
}
return CountAmount--;
}

function StartRace()
{
    RaceStarted = 1; //
    ForEach(i, MAX_PLAYERS)
    {
        if(Joined[i] == true)
        {
            SetVehicleHealth(CreatedRaceVeh[i],1000.0);
            TogglePlayerControllable(i, true);
            SetTimer("UnlockTestRace", 1000, 0);
            PlayerPlaySound(i, 1057, 0.0, 0.0, 0.0);
            GameTextForPlayer(i, "~r~~h~GO GO GO!", 2000, 5);
            SetCameraBehindPlayer(i);
        }
    }
    rCounter = SetTimer("RaceCounter", 1000, 1);
     RaceTick = GetTickCount();
    KillTimer(CountTimer);
    return 1;
}

function UnlockTestRace()
{
    ForEach(i, MAX_PLAYERS)
    {
        if(Joined[i] == true && RaceStarted == 1)
        {
            TogglePlayerControllable(i, true);
            SetCameraBehindPlayer(i);
        }
    }
 return 1;
}

function StopRace(started)
{
    if(started == 0)
    {
        ForEach(i, MAX_PLAYERS)
        {
            if(Joined[i] == true)
            {
                TogglePlayerControllable(i, true);
                SetCameraBehindPlayer(i);
            }
        }
    }
    //KillTimer(TimerCountRace);
    KillTimer(CountTimer);
    KillTimer(rCounter);
    KillTimer(RaceAutoTimer);
    RaceStarted = 0;
    RaceTick = 0;
    RaceBusy = 0x00;
    JoinCount = 0;
    FinishCount = 0;
    TimeProgress = 0;
    ForEach(i, MAX_PLAYERS)
    {
        if(Joined[i] == true)
        {
            DisablePlayerRaceCheckpoint(i);
            DestroyVehicle(CreatedRaceVeh[i]);
            Joined[i] = false;
            OnRace[i] = false;
            TextDrawHideForPlayer(i, RaceInfo[i]);
            CPProgess[i] = 0;
            KillTimer(InfoTimer[i]);
            SetPlayerPos(i, AdminCoords[i][15][ax], AdminCoords[i][15][ay], AdminCoords[i][15][az]);
            SetPlayerInterior(i, AdminCoords[i][15][Interior]);
            SetPlayerVirtualWorld(i, 0);
            RaceSpawnPos[i] = 0;
        }
    }
    if(AutomaticRace==true)
        RaceAutoTimer = SetTimer("RaceAutoStartTimer", AUTO_START_TIMER, 0);
    return 1;
}

function RaceCounter()
{
    if(RaceStarted == 1)
    {
        RaceTime--;
        if(JoinCount <= 0)
        {
            SendClientMessageToAll(COLOR_RACE, "* Гонка завершена, т.к. не осталось участников.");
            StopRace(1);
        }
    }
    if(RaceTime <= 0)
    {
        SendClientMessageToAll(COLOR_RACE, "* Гонка завершена, т.к. истекло время заезда.");
        StopRace(1);
    }
    return 1;
}

