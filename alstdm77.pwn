//-------------------------------[Система дуэлей: /duel]------------------------
// SelectDuelWeapon(playerid) - выбор оружия для дуэли
// StartDuel(duelid) - запуск дуэли
// EndDuel(duelid, reason) - конец дуэли
// IsPlayerNowAtDuel(playerid) - проверка на то, на дуэли ли человек

function ChoiseDuel(playerid)
{
    if(IsPlayerNowAtDuel(playerid)) 
        return SendClientMessage(playerid, COLOR_SYSTEM, "[Ошибка!] Вы уже на дуэли.");

    if(gPlayerSpawned[playerid] == 0) 
        return SendClientMessage(playerid, COLOR_SYSTEM, "[Ошибка!] Сначала необходимо заспавниться.");

    if(jailed[playerid] != 0) 
        return SendClientMessage(playerid, COLOR_SYSTEM, "[Ошибка!] Данная команда недоступна для заключенных.");

    if(DM[playerid][ad] == 1 || DM[playerid][bd] == 1 || DM[playerid][cd] == 1 || DM[playerid][dd] == 1 || DM[playerid][ed] == 1 || DM[playerid][fd] == 1)
        return SendClientMessage(playerid, COLOR_SYSTEM, "[Ошибка!] Сначала необходимо покинуть зону DM командой /exit.");

    if(IsPlayerInRace(playerid)) 
        return SendClientMessage(playerid, COLOR_SYSTEM, "[Ошибка!] Сначала необходимо покинуть гонку командой /exit.");

    new Inter = GetPlayerInterior(playerid);
    if(Inter == 1 || Inter == 4 || Inter == 6 || Inter == 7 || Inter == 10 || Inter == 9 || Inter == 5) 
        return SendClientMessage(playerid, COLOR_SYSTEM, "[Ошибка!] Эту команду нельзя использовать из данного помещения.");

    if(IsPlayerInAnyVehicle(playerid))
        RemovePlayerFromVehicle(playerid);
    new string[1024];
    
    format(string, sizeof(string), "Начать новую дуэль\n");
    format(string, sizeof(string), "%sПодобрать оппонента\n", string);
    for(new i=1; i<11; i++)
    if(Duel[i][Started] == 0 && Duel[i][Countdown] == 0 && Duel[i][SecondSlot] == -1)
    {
        if(Duel[i][FirstSlot] == -1)
            format(string, sizeof(string), "%sАрена №%d (Свободная арена)\n", string, i);
        else
        {
            new weapname[50];
            GetWeaponNameRuAll(Duel[i][SelectedGun], weapname, sizeof(weapname));
            format(string, sizeof(string), "%sАрена №%d (Приглашает %s, оружие: %s)\n", string, i, GetName(Duel[i][FirstSlot]), weapname);
        }
    } 
    else 
    {
        new weapname[50];
        GetWeaponNameRuAll(Duel[i][SelectedGun], weapname, sizeof(weapname));
        format(string, sizeof(string), "%sАрена №%d (%s vs. %s, оружие: %s)\n", string, i, GetName(Duel[i][FirstSlot]), GetName(Duel[i][SecondSlot]), weapname);        
    }
    ShowPlayerDialog(playerid, 2331, DIALOG_STYLE_LIST, "{DCDCDC}= LSTDM 7.5b by NOD-Team | Дуэль =", string, "Выбрать", "Назад");
    return 1;        
}

