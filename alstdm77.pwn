//------------------------------------------------------------------------------
stock GetDistanceBetweenPlayers(playerid, playerid2)
{
    new Float:x1,Float:y1,Float:z1,Float:x2,Float:y2,Float:z2;
    new Float:tmpdis;
    GetPlayerPos(playerid,x1,y1,z1);
    GetPlayerPos(playerid2,x2,y2,z2);
    tmpdis = floatsqroot(floatpower(floatabs(floatsub(x2,x1)),2)+floatpower(floatabs(floatsub(y2,y1)),2)+floatpower(floatabs(floatsub(z2,z1)),2));
    return floatround(tmpdis);
    }
//------------------------------------------------------------------------------
stock CutClanTagFromName(name[])
{
    new tmp[MAX_PLAYER_NAME];
    format(tmp, sizeof(tmp), name);
    new pos1 = strfind(tmp,"[");
    new pos2 = strfind(tmp,"]");
    new pos3 = strfind(tmp,"[",false,1);
    if(pos3>=0 && pos3>pos2 && pos1<pos3)
    {
        if(pos2 >= 0 && (pos2+1 != strlen(tmp)) && (pos1 < pos2)) 
            strmid( tmp, tmp, pos2 + 1, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
    }
    else if((pos3>=0 && pos3>pos1 && pos2>pos3))
    {
        return tmp;
    }
    else if(pos3==-1 || pos3==pos1)
    {
        if(pos2 >= 0 && (pos2+1 != strlen(tmp)) && (pos1 < pos2)) 
            strmid( tmp, tmp, pos2 + 1, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
    }
    return tmp;
}
