public OnPlayerDisconnect(playerid,reason) // #7
{
    if(TaxiDriver[playerid]) 
        TaxiExit(playerid);
    SavePlayerProgress(playerid);
    zoneupdates[playerid] = 0;
    player_zone[playerid] = -1;
    TextDrawHideForPlayer(playerid, Clock);
    TextDrawHideForPlayer(playerid, NODGroup);
    TextDrawHideForPlayer(playerid, RaceInfo[playerid]);
    new string[256];
    GetPlayerName(playerid, string, sizeof(string));
    format(string,sizeof(string), "* %s (ID %d) покинул(а) наш сервер", string, playerid);
    switch(reason) 
    {
        case 0: format(string,sizeof(string), "%s (Потеря связи)", string);
        case 1: format(string,sizeof(string), "%s (Выход)", string);
        case 2: format(string,sizeof(string), "%s (Кик/Бан)", string);
    }
    for(new i = 0; i < MAX_PLAYERS; i++)
        if(IsPlayerConnected(i) && playerid != i)
            SendClientMessage(i, COLOR_SYSTEM, string);
    SetPlayerScore(playerid,0);
    
    if(freeze[playerid]==1)    
        KillTimer(freezetimer[playerid]);
        
    if(AdminVehicle[playerid] != 0)
    {
        DestroyObject(Neon[AdminVehicle[playerid]][LeftNeon]);
        DestroyObject(Neon[AdminVehicle[playerid]][RightNeon]);
        DestroyObject(BFNeon[AdminVehicle[playerid]][0]);
        DestroyObject(BFNeon[AdminVehicle[playerid]][1]);
        DestroyVehicle(AdminVehicle[playerid]);
    }

    if(gTeam[playerid] == TEAM_MEDICS)
    {
        ItemInfo[playerid][Item1n] = 0;
        ItemInfo[playerid][Item1ID] = 0;
    }

    if(Joined[playerid] == true)
    {
        RaceSpawnPos[playerid] = 0;
        Joined[playerid] = false;
        if(OnRace[playerid] == true)
        {
            JoinCount--;
            OnRace[playerid] = false;
            DestroyVehicle(CreatedRaceVeh[playerid]);
            DisablePlayerRaceCheckpoint(playerid);
            TextDrawHideForPlayer(playerid, RaceInfo[playerid]);
            CPProgess[playerid] = 0;
            CPCoords[CPProgess[playerid]][3] = 0;
            KillTimer(InfoTimer[playerid]);
            format(string,sizeof(string),"* %s покинул(а) гонку.",GetName(playerid));
            SendClientMessageToAll(COLOR_RACE,string);
            SetPlayerVirtualWorld(playerid, 0);
        }
    }
    TextDrawDestroy(RaceInfo[playerid]);

    if(IsPlayerNowAtDuel(playerid)) 
        DisconnectFromDuel(playerid);

    KillTimer(PInfo[playerid][Timer]);
    NullVariables(playerid);
    return 1;
}

function SavePlayerProgress(playerid)
{
    new quer[512];
    if(!IsPlayerConnected(playerid) || PInfo[playerid][Logged]==0) 
        return 0;
    if(!strcmp(PInfo[playerid][Marry], "-", true)) 
        PInfo[playerid][MarrySID] = 0;
    format(quer,sizeof(quer),"UPDATE `tdm_players` SET `money`='%d', `kills`='%d', `deaths`='%d', `bounty`='%d', `lvl`='%d',",GetPlayerMoney(playerid),PInfo[playerid][Kills],PInfo[playerid][Deaths],PInfo[playerid][Bounty],PInfo[playerid][LVL]);
    format(quer,sizeof(quer),"%s `races`='%d', `race_wins`='%d', `duels`='%d', `kicks`='%d', `bans`='%d', `rang1`='%d', `rang2`='%d', `rang3`='%d', `score1`='%d', `score2`='%d', `score3`='%d', `bonus_exp`='%d', `muted`='%d', `jailed`='%d', `Item1`='%d', `Item1n`='%d', `Item2`='%d', `Item2n`='%d', `Item3`='%d', `Item3n`='%d'  WHERE `nickname`='%s'",quer,
    PInfo[playerid][Races],
    PInfo[playerid][RacesW],
    PInfo[playerid][Duels],
    PInfo[playerid][Kicks],
    PInfo[playerid][Bans],
    PInfo[playerid][MedRank],
    PInfo[playerid][PoliceRank],
    PInfo[playerid][TaxiRank],
    PInfo[playerid][MedScore],
    PInfo[playerid][PoliceScore],
    PInfo[playerid][TaxiScore],
    PInfo[playerid][BonusEXP],
    muted[playerid],
    jailed[playerid],
    ItemInfo[playerid][Item1ID],
    ItemInfo[playerid][Item1n],
    ItemInfo[playerid][Item2ID],
    ItemInfo[playerid][Item2n],
    ItemInfo[playerid][Item3ID],
    ItemInfo[playerid][Item3n],
    PNickWithoutTag[playerid]);
    mysql_query(quer);
    format(quer,sizeof(quer),"UPDATE `tdm_players` SET `realname`='%s', `city`='%s', `icq`='%s', `skype`='%s', `vkontakte`='%s', `is_female`='%d', `lastnickname`='%s', `marry`='%d' WHERE `nickname`='%s'",
    UserInfo[playerid][Name],UserInfo[playerid][City],UserInfo[playerid][ICQ],UserInfo[playerid][Skype],UserInfo[playerid][VK],UserInfo[playerid][Sex], GetName(playerid), PInfo[playerid][MarrySID], PNickWithoutTag[playerid]);
    mysql_query(quer);
    return 1;
}
