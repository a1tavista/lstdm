TaxiEnter(playerid)
{
    TaxiDriver[playerid] = 1;
    return 1;
}
//------------------------------------------------------------------------------
function TaxiExit(playerid)
{
    new string[256];
    new moneys;
    ForAllPlayers(i)
    {
        if(TaxiPassenger[i] == playerid)
        {
            new driverid = playerid;
            moneys = TotalTaxiCost[i];
            format(string, sizeof(string), "* Вы заплатили %d$ за услуги такси.", moneys);
            SendClientMessage(i, COLOR_TAXI, string);
            format(string, sizeof(string), "* Вы получили %d$ за перевозку клиента %s.", moneys + (moneys * 4 - MIN_TAXI_COST), GetName(i));
            SendClientMessage(playerid, COLOR_TAXI, string);
            A_GivePlayerMoney(i, -moneys);
            A_GivePlayerMoney(driverid, moneys + (moneys * 4 - MIN_TAXI_COST));
            PInfo[driverid][TaxiScore] += 1;
            TaxiPassenger[i] =- 1;
            TotalPassengers--;
            TaxiEXPCount[driverid] = 0;
            if(TotalPassengers==0)
            {
                KillTimer(PayTaxiTimer);
            }
        }
    }
    PassengersInTaxi[playerid] = 0;
    TaxiDriver[playerid] = 0;
    return 1;
}
//------------------------------------------------------------------------------
function TaxiTimer()
{
    ForAllPlayers(i) 
    {
        TotalTaxiCost[i] += TAXI_COST;
        if(TaxiDriver[i] == 1 && PassengersInTaxi[i] >= 1) 
        TaxiEXPCount[i]++;
        {
            if(PassengersInTaxi[i] >= 1 && TaxiEXPCount[i] >= 3) 
                PInfo[i][TaxiScore] += 1;
        }
    }
}
