//------------------------------------------------------------------------------
public OnPlayerRequestSpawn(playerid) // #9
{
    if(PInfo[playerid][Logged] == 0)
        return 0;
    new SkinID = GetPlayerSkin(playerid);
    if(SkinID == 217 || SkinID == 240 || SkinID == 124 || SkinID == 12)
    {
        new Nick[MAX_PLAYER_NAME];
        GetPlayerName(playerid, Nick, sizeof(Nick));
        if(strfind(Nick, "[NOD]", true) != -1 || strfind(Nick, "[NOD_tp]", true) != -1)
            return 1;
        else 
        { 
            SendClientMessage(playerid, COLOR_SYSTEM, "[Ошибка!] Этот класс доступен только представителям NOD-Team."); 
            return 0; 
        }
    }
    else if(SkinID == 98 || SkinID == 258 || SkinID == 295 || SkinID == 40)
    {
        if(PInfo[playerid][LVL] >= 3) 
            return 1;
        else
        {
            SendClientMessage(playerid, COLOR_SYSTEM, "[Ошибка!] Этот класс доступен только игрокам, достигшим 3 LVL."); 
            return 0;
        }
    }
    return 1;
}
